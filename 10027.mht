.scn-loading-spinner { width: 40px; height: 40px; position: relative; margin: 20px auto; }

.scn-loading-spinner .scn-dot { width: 40px; height: 40px; position: absolute; left: 0px; top: 0px; }

.scn-loading-spinner .scn-dot::before { content: ""; display: block; margin: 0px auto; width: 15%; height: 15%; background-color: rgb(240, 171, 0); border-radius: 100%; animation: 1.2s ease-in-out 0s infinite normal both running scn-dotFadeDelay; }

.scn-loading-spinner .scn-dot2 { transform: rotate(30deg); }

.scn-loading-spinner .scn-dot3 { transform: rotate(60deg); }

.scn-loading-spinner .scn-dot4 { transform: rotate(90deg); }

.scn-loading-spinner .scn-dot5 { transform: rotate(120deg); }

.scn-loading-spinner .scn-dot6 { transform: rotate(150deg); }

.scn-loading-spinner .scn-dot7 { transform: rotate(180deg); }

.scn-loading-spinner .scn-dot8 { transform: rotate(210deg); }

.scn-loading-spinner .scn-dot9 { transform: rotate(240deg); }

.scn-loading-spinner .scn-dot10 { transform: rotate(270deg); }

.scn-loading-spinner .scn-dot11 { transform: rotate(300deg); }

.scn-loading-spinner .scn-dot12 { transform: rotate(330deg); }

.scn-loading-spinner .scn-dot2::before { animation-delay: -1.1s; }

.scn-loading-spinner .scn-dot3::before { animation-delay: -1s; }

.scn-loading-spinner .scn-dot4::before { animation-delay: -0.9s; }

.scn-loading-spinner .scn-dot5::before { animation-delay: -0.8s; }

.scn-loading-spinner .scn-dot6::before { animation-delay: -0.7s; }

.scn-loading-spinner .scn-dot7::before { animation-delay: -0.6s; }

.scn-loading-spinner .scn-dot8::before { animation-delay: -0.5s; }

.scn-loading-spinner .scn-dot9::before { animation-delay: -0.4s; }

.scn-loading-spinner .scn-dot10::before { animation-delay: -0.3s; }

.scn-loading-spinner .scn-dot11::before { animation-delay: -0.2s; }

.scn-loading-spinner .scn-dot12::before { animation-delay: -0.1s; }

@-webkit-keyframes scn-dotFadeDelay { 
  0%, 100%, 39% { opacity: 0; }
  40% { opacity: 1; }
}

@keyframes scn-dotFadeDelay { 
  0%, 100%, 39% { opacity: 0; }
  40% { opacity: 1; }
